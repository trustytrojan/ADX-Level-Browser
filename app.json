{
  "expo": {
    "name": "ADX Converts",
    "slug": "adx-convert-browser",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trustytrojan.adx-convert-browser"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.trustytrojan.adxconvertbrowser"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
